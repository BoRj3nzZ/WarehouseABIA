/** @brief package modelo
 */
package modelo;

import java.util.Date;

/** @brief Libraries
 */

/**
 * @brief Class Task
 */
public class Task {

	int id;
	Vehiculo vehiculo;
	Articulos articulo;
	Date fecha;
	String estado;
	
	public Task(int id, Articulos articulo, String estado){
		this.id = id;
		this.articulo = articulo;
		this.estado = estado;
	}
	
	
}
