package modeloTest;

import static org.junit.Assert.assertEquals;

import java.lang.reflect.Method;

import org.junit.Before;
import org.junit.Test;

import modelo.Parking;
import modelo.Posicion;

public class ParkingTest {
	private Parking parking;
	@Before
	public void crearPosicion()
	{
		parking = new Parking(10, "Parking1");
		//System.setOut(new PrintStream(outContent));
	}
	@Test
	public void getPosTest() throws NoSuchMethodException, SecurityException{
		Method method = Parking.class.getDeclaredMethod("getPos");
		method.setAccessible(true);
	    int result = parking.getPos();
	    assertEquals("posocion debe ser 10", 10,(Object)result);
	
	}
	
}
